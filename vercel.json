{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/ask.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/status.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/ask",
      "dest": "/api/ask.py"
    },
    {
      "src": "/api/status",
      "dest": "/api/status.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}